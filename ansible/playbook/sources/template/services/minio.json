{
  "id": "/minio",
  "args": [
    "server",
    "-C",
    "/config",
    "/export"
  ],
  "backoffFactor": 1.15,
  "backoffSeconds": 1,
  "container": {
    "portMappings": [
      {
        "containerPort": 9000,
        "hostPort": 0,
        "protocol": "tcp",
        "servicePort": 9000
      }
    ],
    "type": "DOCKER",
    "volumes": [
      {
        "persistent": {
          "type": "root",
          "size": 1024,
          "constraints": []
        },
        "mode": "RW",
        "containerPath": "miniodata"
      },
      {
        "persistent": {
          "type": "root",
          "size": 10,
          "constraints": []
        },
        "mode": "RW",
        "containerPath": "minioconfig"
      },
      {
        "containerPath": "/export",
        "hostPath": "miniodata",
        "mode": "RW"
      },
      {
        "containerPath": "/config",
        "hostPath": "minioconfig",
        "mode": "RW"
      }
    ],
    "docker": {
      "image": "minio/minio:RELEASE.2017-11-22T19-55-46Z",
      "forcePullImage": true,
      "privileged": true,
      "parameters": []
    }
  },
  "cpus": 1,
  "disk": 0,
  "env": {
    "MINIO_ACCESS_KEY": "{{ minio_user }}",
    "MINIO_SECRET_KEY": "{{ minio_password }}"
  },
  "healthChecks": [
    {
      "gracePeriodSeconds": 120,
      "intervalSeconds": 30,
      "maxConsecutiveFailures": 3,
      "timeoutSeconds": 20,
      "delaySeconds": 15,
      "protocol": "COMMAND",
      "command": {
        "value": "/usr/bin/healthcheck.sh"
      }
    }
  ],
  "instances": 1,
  "labels": {
    "DCOS_PACKAGE_OPTIONS": "eyJzZXJ2aWNlIjp7Im5hbWUiOiJtaW5pbyIsInRscyI6ZmFsc2V9LCJyZXNvdXJjZSI6eyJjcHVzIjoxLCJtZW0iOjEwMjR9LCJzdG9yYWdlIjp7InZvbHVtZS1zaXplIjoxMDI0fSwiY3JlZGVudGlhbHMiOnsiYWNjZXNzLWtleSI6Im1pbmlvIiwic2VjcmV0LWtleSI6IlZvb2hvc29uQWpmaSJ9LCJuZXR3b3JraW5nIjp7InBvcnQiOjkwMDAsInB1YmxpYy1hY2Nlc3MiOnRydWV9fQ==",
    "HAPROXY_0_REDIRECT_TO_HTTPS": "true",
    "HAPROXY_0_MODE": "http",
    "DCOS_PACKAGE_SOURCE": "https://universe.mesosphere.com/repo",
    "HAPROXY_GROUP": "external",
    "DCOS_PACKAGE_METADATA": "",
    "DCOS_PACKAGE_DEFINITION": "",
    "DCOS_PACKAGE_VERSION": "0.0.11-RELEASE.2017-11-22T19-55-46Z",
    "DCOS_PACKAGE_NAME": "minio",
    "MARATHON_SINGLE_INSTANCE_APP": "true",
    "HAPROXY_0_VHOST": "{{ cluster_name }}-minio.{{ cluster_domain }}"
  },
  "maxLaunchDelaySeconds": 36000,
  "mem": 1024,
  "gpus": 0,
  "networks": [
    {
      "mode": "container/bridge"
    }
  ],
  "residency": {
    "relaunchEscalationTimeoutSeconds": 3600,
    "taskLostBehavior": "WAIT_FOREVER"
  },
  "requirePorts": false,
  "upgradeStrategy": {
    "maximumOverCapacity": 0,
    "minimumHealthCapacity": 0
  },
  "killSelection": "YOUNGEST_FIRST",
  "unreachableStrategy": "disabled",
  "fetch": [],
  "constraints": []
}
